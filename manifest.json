{
  "manifest_version": 3,
  "name": "Custom Request Headers",
  "version": "1.0",
  "permissions": ["storage", "declarativeNetRequest"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "action": {
    "default_popup": "public/popup.html"
  },
  "host_permissions": ["<all_urls>"]
}
